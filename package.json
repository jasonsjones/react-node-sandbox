{
  "name": "microservices-api",
  "version": "1.0.0",
  "description": "A sandbox API built on microservices",
  "main": "index.js",
  "scripts": {
    "predev": "npm run dbSeed",
    "dev": "nodemon src/server.js --watch src/ --exec babel-node",
    "dbSeed": "DEBUG=db:* babel-node src/dbSeed.js",
    "lt": "lt --port 3000 --subdomain sandboxapi",
    "test": "NODE_ENV=test mocha src/**/*.test.js --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsjones/sandbox-api.git"
  },
  "keywords": [
    "node",
    "api",
    "microservices"
  ],
  "author": "Jason Jones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsjones/sandbox-api/issues"
  },
  "homepage": "https://github.com/jasonsjones/sandbox-api#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.11.3",
    "morgan": "^1.8.2",
    "multer": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "debug": "^3.0.0",
    "eslint": "^4.4.1",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.0"
  }
}
