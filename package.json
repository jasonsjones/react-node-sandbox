{
  "name": "microservices-api",
  "version": "1.0.0",
  "description": "A sandbox API built on microservices",
  "main": "index.js",
  "scripts": {
    "predev": "npm run dbSeed",
    "dev": "nodemon src/api/server.js --inspect=0.0.0.0:5858 --watch src/ --exec babel-node",
    "prebuild": "rm -f public/bundle.js public/styles/styles.css",
    "build": "webpack -wd",
    "dbSeed": "babel-node src/api/dbSeed.js",
    "lt": "lt --port 3000 --subdomain sandboxapi",
    "lint": "eslint webpack.config.js src/**/*.js",
    "test": "NODE_ENV=test mocha src/api/**/*.test.js --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsjones/react-node-sandbox.git"
  },
  "keywords": [
    "node",
    "api",
    "microservices"
  ],
  "author": "Jason Jones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsjones/react-node-sandbox/issues"
  },
  "homepage": "https://github.com/jasonsjones/react-node-sandbox#readme",
  "dependencies": {
    "@salesforce-ux/design-system": "^2.3.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "express-graphql": "^0.6.7",
    "express-session": "^1.15.5",
    "flux": "^3.1.3",
    "graphql": "^0.10.5",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.11.3",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "node-fetch": "^1.7.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.7",
    "debug": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.0",
    "sinon-mongoose": "^2.0.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  }
}
